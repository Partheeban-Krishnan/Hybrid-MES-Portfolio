{
  "title": "Digital Twin Dashboard",
  "uid": "digital-twin-dashboard",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "awsiot-twinmaker-scene",
      "title": "Factory 3D Scene",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 15
      },
      "options": {
        "workspaceId": "DigitalTwin",
        "sceneId": "Factory3DScene",
        "selectedEntityId": "FactoryEntity"
      },
      "datasource": {
        "type": "awsiot-twinmaker-datasource",
        "uid": "YOUR_TWINMAKER_DATASOURCE_UID"
      }
    },
    {
      "type": "video",
      "title": "Live Factory Feed",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 15
      },
      "options": {
        "source": "url",
        "url": "http://<EC2-public-IP>:8080/stream.m3u8",
        "autoplay": true,
        "loop": true
      }
    },
    {
      "type": "stat",
      "title": "Temperature",
      "gridPos": {
        "x": 0,
        "y": 15,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "awsiot-twinmaker-datasource",
            "uid": "YOUR_TWINMAKER_DATASOURCE_UID"
          },
          "queryType": "property",
          "workspaceId": "DigitalTwin",
          "entityId": "FactoryEntity",
          "componentName": "SensorComponent",
          "propertyName": "temperature"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius"
        }
      }
    },
    {
      "type": "stat",
      "title": "Pressure",
      "gridPos": {
        "x": 6,
        "y": 15,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "datasource": {
            "type": "awsiot-twinmaker-datasource",
            "uid": "YOUR_TWINMAKER_DATASOURCE_UID"
          },
          "queryType": "property",
          "workspaceId": "DigitalTwin",
          "entityId": "FactoryEntity",
          "componentName": "SensorComponent",
          "propertyName": "pressure"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bar"
        }
      }
    }
  ]
}